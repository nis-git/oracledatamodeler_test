   <userDefinedSQL>CREATE OR REPLACE VIEW PROFITS&amp;lt;br&amp;gt;AS &amp;lt;br&amp;gt;SELECT&amp;lt;br&amp;gt;	s.channel_id,&amp;lt;br&amp;gt;	s.cust_id,&amp;lt;br&amp;gt;	s.prod_id,&amp;lt;br&amp;gt;	s.promo_id,&amp;lt;br&amp;gt;	s.time_id,&amp;lt;br&amp;gt;	c.unit_cost,&amp;lt;br&amp;gt;	c.unit_price,&amp;lt;br&amp;gt;	s.amount_sold,&amp;lt;br&amp;gt;	s.quantity_sold,&amp;lt;br&amp;gt;	c.unit_cost * s.quantity_sold TOTAL_COST&amp;lt;br&amp;gt; FROM&amp;lt;br&amp;gt;	costs c, sales s&amp;lt;br&amp;gt; WHERE c.prod_id = s.prod_id&amp;lt;br&amp;gt;   AND c.time_id = s.time_id&amp;lt;br&amp;gt;   AND c.channel_id = s.channel_id&amp;lt;br&amp;gt;   AND c.promo_id = s.promo_id</userDefinedSQL>
