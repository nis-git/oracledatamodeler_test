<userDefinedSQL>CREATE OR REPLACE VIEW QUANTITIES_SOLD&lt;br&gt;AS &lt;br&gt;SELECT &quot;SH&quot;.&quot;COUNTRIES&quot;.&quot;COUNTRY_NAME&quot;, &quot;SH&quot;.&quot;PRODUCTS&quot;.&quot;PROD_NAME&quot;, &quot;SH&quot;.&quot;TIMES&quot;.&quot;CALENDAR_YEAR&quot;, &quot;SH&quot;.&quot;CUSTOMERS&quot;.&quot;CUST_LAST_NAME&quot;, &quot;SH&quot;.&quot;SALES&quot;.&quot;QUANTITY_SOLD&quot; FROM &quot;SH&quot;.&quot;COUNTRIES&quot;, &quot;SH&quot;.&quot;PRODUCTS&quot;, &quot;SH&quot;.&quot;TIMES&quot;, &quot;SH&quot;.&quot;CUSTOMERS&quot;, &quot;SH&quot;.&quot;SALES&quot; WHERE ( &quot;SH&quot;.&quot;CUSTOMERS&quot;.&quot;COUNTRY_ID&quot; = &quot;SH&quot;.&quot;COUNTRIES&quot;.&quot;COUNTRY_ID&quot; ) AND ( &quot;SH&quot;.&quot;SALES&quot;.&quot;CUST_ID&quot; = &quot;SH&quot;.&quot;CUSTOMERS&quot;.&quot;CUST_ID&quot; ) AND ( &quot;SH&quot;.&quot;SALES&quot;.&quot;PROD_ID&quot; = &quot;SH&quot;.&quot;PRODUCTS&quot;.&quot;PROD_ID&quot; ) AND ( &quot;SH&quot;.&quot;SALES&quot;.&quot;TIME_ID&quot; = &quot;SH&quot;.&quot;TIMES&quot;.&quot;TIME_ID&quot; )</userDefinedSQL>
